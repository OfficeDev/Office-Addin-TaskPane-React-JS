{
  "name": "office-addin-taskpane-react-js",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane-React-JS.git"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "excel",
    "app-type-to-debug": "desktop",
    "dev-server-port": 3000
  },
  "scripts": {
    "build": "webpack -p --mode production --https false",
    "build:dev": "webpack --mode development --https false",
    "build-dev": "webpack --mode development --https false && echo . && echo . && echo . && echo Please use 'build:dev' instead of 'build-dev'.",
    "convert-to-single-host": "node convertToSingleHost.js",
    "dev-server": "webpack-dev-server --mode development",
    "sideload": "echo Please specify which app using 'npm run sideload:app'.",
    "sideload:excel": "office-toolbox sideload -m manifest.xml -a excel",
    "sideload:powerpoint": "office-toolbox sideload -m manifest.xml -a powerpoint",
    "sideload:word": "office-toolbox sideload -m manifest.xml -a word",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "test": "mocha -r ts-node/register test/*.ts",
    "unload": "echo Please specify which app using 'npm run unload:app'.",
    "unload:excel": "office-toolbox remove -m manifest.xml -a excel",
    "unload:powerpoint": "office-toolbox remove -m manifest.xml -a powerpoint",
    "unload:word": "office-toolbox remove -m manifest.xml -a word",
    "validate": "office-toolbox validate -m manifest.xml",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "core-js": "^3.1.4",
    "es6-promise": "^4.2.8",
    "office-ui-fabric-react": "^7.10.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@types/es6-collections": "^0.5.29",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.3",
    "@types/office-js": "^1.0.19",
    "@types/office-runtime": "^1.0.8",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack": "^4.4.34",
    "@types/webpack-dev-server": "^3.1.6",
    "babel-polyfill": "^6.26.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "current-processes": "^0.2.1",
    "css-loader": "^3.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mocha": "^6.1.4",
    "office-addin-debugging": "^2.1.13",
    "office-addin-dev-certs": "^1.0.1",
    "office-addin-test-helpers": "^0.3.1",
    "office-addin-test-server": "^0.3.1",
    "office-toolbox": "^0.2.1",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^4.12.6",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
